<!DOCTYPE HTML>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Developer mode</title>

    <!-- Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-145917564-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-145917564-1');
    </script>


    @Styles.Render("https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css")
    @Styles.Render("https://use.fontawesome.com/releases/v5.7.2/css/all.css")
    @RenderSection("Css", required: false)
    @Styles.Render("~/Content/css/main.min.css")


</head>

    <body data-spy="scroll" data-target="#main-navbar" data-offset="200">
        @RenderBody()
    </body>

    @RenderSection("JavascriptDependencies", required: false)
    @Scripts.Render("https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js")
    @Scripts.Render("https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js")
    @Scripts.Render("https://cdnjs.cloudflare.com/ajax/libs/jquery.nicescroll/3.7.6/jquery.nicescroll.min.js")
    @RenderSection("JavascriptInnerDependencies", required: false)
    @Scripts.Render("~/Content/scripts/main.js")
    @RenderSection("Javascript", required: false)
</html>